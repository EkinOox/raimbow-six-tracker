{
  "name": "r6-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test:quick": "node quick-test.js",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:headless": "cypress run --headless",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:navigation": "cypress run --spec 'cypress/e2e/pages-navigation.cy.ts'",
    "test:functionality": "cypress run --spec 'cypress/e2e/functionality.cy.ts'",
    "test:auth": "cypress run --spec 'cypress/e2e/auth.cy.ts'",
    "test:all": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "optimize:images": "node scripts/resize-images.js --dir public/images --max-edge 1600 --quality 80 --in-place --verbose",
    "optimize:images:dry": "node scripts/resize-images.js --dir public/images --max-edge 1600 --quality 80 --dry-run --verbose"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^12.23.22",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.552.0",
    "mongoose": "^8.19.2",
    "next": "15.5.4",
    "next-auth": "^5.0.0-beta.30",
    "primeicons": "^7.0.0",
    "r6-data.js": "^1.3.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^15.5.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "sharp": "^0.34.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
